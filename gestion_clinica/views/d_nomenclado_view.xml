<odoo>
    <!-- Vista de formulario para el modelo DiagnosticoNomenclado -->
    <record id="view_diagnostico_nomenclado_form" model="ir.ui.view">
        <field name="name">Diagnóstico Nomenclado Form</field>
        <field name="model">clinica.d_nomenclado</field>
        <field name="arch" type="xml">
            <form string="Diagnóstico Nomenclado">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="descripcion"/>
                        <field name="pacientes_ids"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Vista de árbol para el modelo DiagnosticoNomenclado -->
    <record id="view_diagnostico_nomenclado_tree" model="ir.ui.view">
        <field name="name">Diagnóstico Tree</field>
        <field name="model">clinica.d_nomenclado</field>
        <field name="arch" type="xml">
            <tree string="Diagnósticos">
                <field name="name"/>
                <field name="pacientes_ids"/>
            </tree>
        </field>
    </record>
</odoo>

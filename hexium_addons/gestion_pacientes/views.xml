<odoo>
<record id="view_tree_gestion_pacientes" model="ir.ui.view">
    <field name="name">view.tree.gestion_pacientes</field>
    <field name="model">pacientes</field>
    <field name="arch" type="xml">
        <tree>
            <field name="nombre"/>
            <field name="apellido"/>
            <field name="tipo_documento"/>
            <field name="numero_documento"/>
            <field name="fecha_nacimiento"/>
            <field name="genero"/>
            <field name="direccion"/>
            <field name="ciudad"/>
            <field name="codigo_postal"/>
            <field name="telefono"/>
        </tree>
    </field>

</record>

</odoo>